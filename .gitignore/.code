   #region MultiLanguage
        FileIniDataParser parser = new FileIniDataParser();
        IniData inidata;
        public void iniDosyaislemeleriComboboxtanDilleriDoldur(ComboBox cmbLanguageSelection)
        {
            //okumak icin:
            inidata = parser.ReadFile("MultiLanguage.ini");
            //string useFullScreenStr = data["General"]["fullscreen"];
            //bool useFullScreen = bool.Parse(useFullScreenStr); //useFullScreenStr = "true"

            int a = inidata.Sections.Count;


            cmbLanguageSelection.Items.Clear();
            foreach (SectionData sd in inidata.Sections.ToList())
            {
                string i = sd.SectionName;
                cmbLanguageSelection.Items.Add(i.ToUpper());
            }
            if (cmbLanguageSelection.Items.Count >= 1)
            {
                cmbLanguageSelection.SelectedIndex = 0;
            }

        }


        public void getAllCurrentCulterName()
        {

            string path = @"C:\CulterInfo\AllCulterInfo.txt";
            Directory.CreateDirectory(@"C:\CulterInfo");

            if (File.Exists(path)==false)
            {
             
                using (System.IO.StreamWriter fs = new System.IO.StreamWriter(path, true))
                {
                    // Add some information to the file.
                    
                    CultureInfo[] cultures = CultureInfo.GetCultures(CultureTypes.NeutralCultures);

                    foreach (var culture in cultures)
                    {
                        string CulterInfoTXT = culture.TextInfo.CultureName + "_____" + culture.DisplayName;

                        fs.WriteLine(CulterInfoTXT);
                    }

                    fs.Close();
                }
            }
        

        }

        public enum iniBuyukKucukKontrol
        {
            TamamiBuyuk, TamamiKucuk, IlkHarflerBuyukDigerleriKucuk
        }

        public string iniDataGetValue(string aranan, iniBuyukKucukKontrol gelen)
        {

            if (inidata[Dil][aranan] == null || inidata[Dil][aranan] == "")
            {
                return aranan;
            }
            else
            {
                switch (gelen)
                {
                    case  iniBuyukKucukKontrol.TamamiBuyuk:
                        if (inidata[Dil]["CultureName"] != null)
                        {
                            return CultureInfo.GetCultureInfo(inidata[Dil]["CultureName"]).TextInfo.ToUpper(inidata[Dil][aranan].ToString());

                        }
                        return CultureInfo.CurrentCulture.TextInfo.ToUpper(inidata[Dil][aranan].ToString());

                    case  iniBuyukKucukKontrol.TamamiKucuk:

                        if (inidata[Dil]["CultureName"] != null)
                        {
                            return CultureInfo.GetCultureInfo(inidata[Dil]["CultureName"]).TextInfo.ToLower(inidata[Dil][aranan].ToString());

                        }
                        return CultureInfo.CurrentCulture.TextInfo.ToLower(inidata[Dil][aranan].ToString());


                    case  iniBuyukKucukKontrol.IlkHarflerBuyukDigerleriKucuk:


                        if (inidata[Dil]["CultureName"] != null)
                        {
                            return CultureInfo.GetCultureInfo(inidata[Dil]["CultureName"]).TextInfo.ToTitleCase(inidata[Dil][aranan].ToString());

                        }
                        return CultureInfo.CurrentCulture.TextInfo.ToTitleCase(inidata[Dil][aranan].ToString());

                    default:
                        return inidata[Dil][aranan].ToString();

                }
            }
        } 
        #endregion




